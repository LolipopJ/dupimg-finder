{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:tailwindcss/recommended",
    "next",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "settings": {
    "next": {
      "rootDir": "renderer/"
    }
  },
  "rules": {
    "tailwindcss/no-custom-classname": 0
  }
}
